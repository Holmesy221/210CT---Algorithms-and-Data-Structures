python: 

def reverse(sentence):
    """ Funtion to reverse words in a sentance """
    words = sentence.split(" ")  
    result = ""                        
    for i in range(len(words) - 1, -1, -1): # iterate over words starting from (last)reverse order
        result += words[i]                  # add words to result
        if i != 0: 
            result += " "                   # adds space if not last 
    return result


print(reverse("Lee is here")) # test



